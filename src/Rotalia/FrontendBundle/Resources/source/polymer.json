{
  "entrypoint": "index.html",
  "shell": "elements/kassa-app.html",
  "fragments": [
    "elements/kassa-login.html",
    "elements/kassa-logout.html",
    "elements/kassa-ost.html",
    "elements/kassa-admin-tooted.html",
    "elements/kassa-admin-tootegrupid.html",
    "elements/kassa-admin-kassad.html"
  ],
  "sources": [
    "elements/*",
    "js/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.js"
  ],
  "builds": [
    {
      "preset": "es5-bundled",
      "name": "default",
      "addServiceWorker": false
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  }
}
