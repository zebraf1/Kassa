{
  "entrypoint": "index.html",
  "shell": "elements/kassa-app.html",
  "fragments": [],
  "sources": [
    "elements/*",
    "js/*",
    "images/**/*",
    "index.html",
    "elements/kassa-app.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.js"
  ],
  "builds": [
    {
      "name": "default",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es5-bundled"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ]
  }
}